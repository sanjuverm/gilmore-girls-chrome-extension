{
  "manifest_version": 2,

  "name": "Gilmore-ning",
  "description": "Gilmore Girls themed chrome extension, supplying you with a cup of coffee, book, and quote",
  "version": "0.0.0.1",
  "background" : {
     "scripts": [ "background.js" ]
  },
  "browser_action": {
	   "default_icon": "icon.png"
  },

  "chrome_url_overrides" : {
    "newtab" : "index.html"
  },

  "permissions": [
   "activeTab",
   "tabs"
   ]
}
